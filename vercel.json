{
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.5.2"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/styles/style.css", "dest": "/css/style.css" },
      { "src": "/scripts/apply.js", "dest": "/scripts/ref-num.js" }]
  }