{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.php" },
    { "src": "/api/about", "dest": "/api/about.php" },
    { "src": "/api/jobs", "dest": "/api/jobs.php" },
    { "src": "/api/search-eoi", "dest": "/api/search-eoi.php" },
    { "src": "/api/list-eoi", "dest": "/api/list-eoi.php" },
    { "src": "/api/change-eoi", "dest": "/api/change-eoi.php" },
    { "src": "/api/delete-eoi", "dest": "/api/delete-eoi.php" },
    { "src": "/api/manage", "dest": "/api/manage.php" },

    { "src": "/styles/style.css", "dest": "/styles/style.css" },
    { "src": "/scripts/apply.js", "dest": "/scripts/apply.js" },
    { "src": "/scripts/ref-num.js", "dest": "/scripts/ref-num.js" }
  ]
}
